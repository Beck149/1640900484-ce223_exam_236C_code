<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Show by ID</title>
</head>
<body>
    <h1>หน้าแรก</h1>
    <table id="table">
        <tr>
            <th>id</th>
            <th>draw</th>
            <th>no.</th>
            <th>Category</th>
            <th>date</th>
            <th>prize</th>
        </tr>
        <% thh.forEach(function(item) { %>
        <tr>
            <td><%= item.id %></td>
            <td><%= item.draw %></td>
            <td>[ <%= item.no %> ]</td>
            <td><%= item.Category %></td>
            <td><%= item.date %></td>
            <td><%= item.prize %></td>
        </tr>
        <% }) %>
    </table>
</body>
</html>